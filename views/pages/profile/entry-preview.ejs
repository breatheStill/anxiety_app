  <div class="journal preview" id="<%= `preview-${journal.id}` %>" style="border:3px solid <%= rgGradient[journal.sentiment] %>">
    <h4><em>
        <%= journal.date.toDateString() %></em></h4>

    <hr>

    <p class="entry">
      <%- journal.entry.replace(/\n/g, '<br>').split(' ').slice(0, 10).join(' ') + '...' %>
    </p>
  </div>
